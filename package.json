{
  "name": "emufc-api",
  "version": "2.0.0",
  "engines": {
    "node": "8.11.1"
  },
  "private": true,
  "scripts": {
    "start": "node ./server/src/server.js",
    "dev": "NODE_ENV=development DEBUG=emufc-api:* nodemon ./server/src/server.js",
    "lint": "eslint server/src/**/*.js",
    "migrate": "npm run build && sequelize db:migrate",
    "test": "NODE_ENV=test mocha --opts server/test/integration/mocha.opts server/test/integration/**/*.js",
    "loadtest": "NODE_ENV=production loadtest http://localhost:3000/equipments -t 30 -c 12 --rps 100"
  },
  "author": "Icaro Tavares <icarotavares@live.com>",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^2.0.1",
    "bluebird": "^3.5.1",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "debug": "^3.0.0",
    "express": "^4.15.4",
    "helmet": "^3.8.1",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.2.1",
    "morgan": "~1.9.0",
    "mysql2": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "ramda": "^0.25.0",
    "sequelize": "^4.6.0",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "apidoc": "^0.17.6",
    "chai": "^4.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "loadtest": "^3.0.3",
    "mocha": "^5.1.1",
    "nodemon": "^1.11.0",
    "sequelize-cli": "^4.0.0",
    "supertest": "^3.0.0",
    "testdouble": "^3.2.4"
  }
